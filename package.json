{
  "name": "aws4-axios",
  "version": "0.0.0-development",
  "description": "Axios request interceptor for signing requests with AWSv4",
  "author": "James Bourne",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesmbourne/aws4-axios.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jamesmbourne/aws4-axios",
  "bugs": {
    "url": "https://github.com/jamesmbourne/aws4-axios/issues"
  },
  "keywords": [
    "aws4",
    "awsv4",
    "signature",
    "axios",
    "interceptor"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-it": "jest --config jest.config.it.js",
    "test-it:deploy": "cd src/__tests__ && serverless deploy",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "lint:eslint": "eslint . --cache --ext .ts,.tsx",
    "lint:prettier": "prettier --check src/**/*.ts",
    "lint": "run-s lint:*"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.4.1",
    "@aws-sdk/client-sts": "^3.4.1",
    "@types/aws4": "^1.11.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.6.7",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^15.0.0",
    "nock": "^13.3.0",
    "npm-run-all2": "^5.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^22.0.0",
    "serverless": "^3.30.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "zod": "^3.21.4"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.4.1",
    "aws4": "^1.12.0"
  },
  "peerDependencies": {
    "axios": ">=1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "workspaces": [
    "infra"
  ]
}
